@using System.Web.Optimization
@using WZAutoEye.WZAutoEyeMVC.Model
@using WZAutoEye.WZAutoEyeMVC.Utility
@{
    ViewBag.Title = CommonResourceManager.Current.GetString("抱歉，出错了");
    Layout = "";
}
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/Custom/jquery")
@Scripts.Render("~/Scripts/Custom/jquery.Huploadify.js")

<!DOCTYPE html>
<html lang="zh-cmn-Hans">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=0">
    <title>抱歉，出错了</title>
    <link rel="stylesheet" href="../../Scripts/weui.css" />
    <link rel="stylesheet" href="../../Scripts/example.css" />
</head>
<body ontouchstart>
    <div class="weui-toptips weui-toptips_warn js_tooltips">抱歉，出错了</div>
    <div class="container" id="container"></div>
    <div class="page">
        <div class="weui-msg">
            <div class="weui-msg__icon-area"><i class="weui-icon-warn weui-icon_msg"></i></div>
            <div class="weui-msg__text-area">
                <h2 class="weui-msg__title">@ViewBag.info</h2>
            </div>
        </div>
    </div>
    <script src="../../Scripts/zepto.min.js"></script>
    <script type="text/javascript" src="../../Scripts/jweixin-1.0.0.js"></script>
    <script src="../../Scripts/1.0.0/weui.min.js"></script>
    <script src="../../Scripts/example.js"></script>
</body>
</html>

